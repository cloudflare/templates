		"publish": true
	},
	"devDependencies": {
		"typescript": "5.9.3",
		"wrangler": "4.51.0"
	},
	"private": true,
	"scripts": {
		"cf-typegen": "wrangler types",
		"check": "tsc && wrangler deploy --dry-run",
