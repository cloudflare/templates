{
	"name": "astro-blog-starter-template",
	"compatibility_date": "2025-10-08",
	"compatibility_flags": ["nodejs_compat"],
	"main": "./dist/_worker.js/index.js",
	"assets": {
		"directory": "./dist",
		"binding": "ASSETS"
	},
	"d1_databases": [
		{
			"binding": "DB",
			"database_name": "astro-blog-db",
			"database_id": "preview",
			"migrations_dir": "migrations"
		}
	],
	"kv_namespaces": [
		{
			"binding": "DRAFTS",
			"id": "preview"
		}
	],
	"observability": {
		"enabled": true
	},
	"upload_source_maps": true
}
